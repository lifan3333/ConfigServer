<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <import resource="config-server-dataSource.xml"/>
    <bean id="categoryDao" class="com.cmbc.configserver.core.dao.impl.CategoryDaoImpl">
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>
    <bean id="configChangeLogDao" class="com.cmbc.configserver.core.dao.impl.ConfigChangeLogDaoImpl">
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>
    <bean id="configDao" class="com.cmbc.configserver.core.dao.impl.ConfigDetailsDaoImpl">
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>
    <bean id="heartBeatDao" class="com.cmbc.configserver.core.dao.impl.ConfigHeartBeatDaoImpl">
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>
</beans>